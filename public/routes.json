{
  "routes": [
    {
      "route": "/login",
      "serve": "/.auth/login/aad"
    },
    {
      "route": "/logout",
      "serve": "/.auth/logout"
    },
    {
      "route": "/.auth/login/aad",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/facebook",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/twitter",
      "statusCode": "404"
    },
    {
      "route": "/.auth/login/google",
      "statusCode": "404"
    },
    {
      "route": "/api/GetSASToken",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/api/GetGitHubAvatar",
      "allowedRoles": ["authenticated"]
    },
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ]
}
